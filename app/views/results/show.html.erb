<div class='destination' style="color: #808782">
  			<h2>HOBO Results</h2>
</div>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href="#collapse1">
        Hobo Hotel</a>
      </h4>
    </div>
	    <div id="collapse1" class="panel-collapse collapse">
	      <div class="panel-body">
	      	
	      	<% @hotels.each do |hotel| %>
				<div>
					<%= hotel.name %>
					<hr>
					<%= hotel.snippet_text %>
					<hr>
					<img src= <%= hotel.image_url %> >
					<hr>
					<img src= <%= hotel.rating_img_url_large%> >
					<hr>
					<%= hotel.display_phone %>

				</div>
			 	
			 	<div>
			 		<% hotel.location.display_address.each do |address_part| %>
			 		<%= address_part %>
			 			<br>
					<% end %>	
				</div>
			<% end	%>

	      </div>
	    </div>
 
